{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes ": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/posts",
      "dest": "/posts"
    },

    {
      "src": "/posts/:id",
      "dest": "/posts/:id"
    }
  ]
}
